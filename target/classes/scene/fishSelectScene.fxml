<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.tkfisch.fishSelectSceneController">
   <children>
      <Label layoutX="121.0" layoutY="43.0" text="CHOOSE THE FISH" textFill="#0408fa">
         <font>
            <Font name="Book Antiqua Bold" size="40.0" />
         </font>
      </Label>
      <ImageView fx:id="blueFishImage" fitHeight="146.0" fitWidth="147.0" layoutX="14.0" layoutY="156.0" onMouseEntered="#blueFishEntered" onMouseExited="#blueFishExited" onMousePressed="#blueFishPressed" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../image/blueFishReleased.png" />
         </image>
      </ImageView>
      <ImageView fx:id="orangeFishImage" fitHeight="125.0" fitWidth="111.0" layoutX="177.0" layoutY="168.0" onMouseEntered="#orangeFishEntered" onMouseExited="#orangeFishExited" onMousePressed="#orangeFishPressed" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../image/orangeFishReleased.jpg" />
         </image>
      </ImageView>
      <ImageView fx:id="pinkFishImage" fitHeight="125.0" fitWidth="122.0" layoutX="300.0" layoutY="176.0" onMouseEntered="#pinkFishEntered" onMouseExited="#pinkFishExited" onMousePressed="#pinkFishPressed" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../image/pinkFishReleased.jpg" />
         </image>
      </ImageView>
      <ImageView fx:id="yellowFishImage" fitHeight="113.0" fitWidth="146.0" layoutX="449.0" layoutY="185.0" onMouseEntered="#yellowFishEntered" onMouseExited="#yellowFishExited" onMousePressed="#yellowFishPressed" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../image/yellowFishReleased.jpg" />
         </image>
      </ImageView>
   </children>
</AnchorPane>
