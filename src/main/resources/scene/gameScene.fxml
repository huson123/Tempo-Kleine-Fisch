      </Label>
      <ImageView fx:id="dice" fitHeight="143.0" fitWidth="124.0" layoutX="509.0" layoutY="223.0" onMouseEntered="#diceEntered" onMouseExited="#diceExited" onMousePressed="#dicePressed" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../image/dice.png" />
         </image>
      </ImageView>
      <ImageView fx:id="ship" fitHeight="57.0" fitWidth="64.0" layoutX="80.0" layoutY="198.0" pickOnBounds="true" preserveRatio="true">
