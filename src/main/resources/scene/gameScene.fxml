      </ImageView>
      <ImageView fx:id="pinkFish" fitHeight="32.0" fitWidth="32.0" layoutX="250.0" layoutY="244.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../image/pinkFishReleased.jpg" />
         </image>
      </ImageView>
      <ImageView fx:id="yellowFish" fitHeight="32.0" fitWidth="32.0" layoutX="250.0" layoutY="258.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../image/yellowFishReleased.jpg" />
         </image>
      </ImageView>
   </children>
</AnchorPane>
