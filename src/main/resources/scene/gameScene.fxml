            <Image url="@../image/shipReleased.jpg" />
         </image>
      </ImageView>
      <ImageView fx:id="blueFish" fitHeight="32.0" fitWidth="32.0" layoutX="250.0" layoutY="216.0" pickOnBounds="true" preserveRatio="true">
